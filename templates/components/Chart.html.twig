<div
    {{ attributes }}
    data-controller="chart"
    data-action="filter:update@window->chart#updateFilter"
    data-chart-url-value="{{ this.chartHelper.baseUrl }}"
    data-chart-chart-value="{{ this.chartId }}"
    data-chart-station-value="{{ this.station is not null ? this.station.id : null }}"
    data-chart-fuel-value="{{ this.fuel }}"
>
    <div data-chart-target="chart" style="height: 600px;"></div>
</div>
